
.content {
	background: #fff;
}
.tabBox>text {
	vertical-align: bottom
}
.Newcat {
	overflow: hidden;
}
.Newcat-banner>image {
	display: block;
	width: 100%;
	height: 300rpx;
}
.Newcat-list-item>image {
	width: 250rpx;
	height: 250rpx;
}
.Newcat-list-item-pr {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	padding-left: 70rpx;
	padding-top: 20rpx;
	overflow: hidden;
}
.Newcat-list:last-child {
	padding-top: 0;
}
.Newcat-cat-but {
	display: inline-block;
}
.goods-list-box {
	margin-top: 20rpx;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
}
.goods-list-item-box {
	width: 50%;
	margin-bottom: 20rpx;
}
.goods-list-item-box:nth-child(2n+1) .goods-list-item-wrap {
	margin-left: 20rpx;
	margin-right: 10rpx;
}
.goods-list-item-box:nth-child(2n) .goods-list-item-wrap {
	margin-left: 10rpx;
	margin-right: 20rpx;
}
.goods-list-item-wrap {
	/* height: 450rpx; */
	background: #fff;
	overflow: hidden;
	-webkit-border-radius: 16rpx;
	        border-radius: 16rpx;
}
.goods-list-item-wrap view:nth-child(1) image {
	height: 330rpx;
}

